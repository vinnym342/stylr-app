<div class='nav'>
  <% if user_signed_in? %>
  <%= link_to('Logout', destroy_user_session_path, method: :delete) %>
  <%= form_for current_user, url: '/myprofile',method: :get do |f| %>
    <%= f.button '<i class="fa fa-user" aria-hidden="true"></i>'.html_safe %>
  <% end %>
  <% else %>
  <%= link_to('Login', new_user_session_path)  %>
  <% end %>
</div>
